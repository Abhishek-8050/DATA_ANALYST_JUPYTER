    "nums = [10, 25, 8, 40, 15]\n",
    "\n",
    "largest = nums[0]\n",
    "second = nums[0]\n",
    "\n",
    "for num in nums:\n",
    "    if num > largest:\n",
    "        second = largest\n",
    "        largest = num\n",
    "    elif num > second and num != largest:\n",
    "        second = num\n",
    "\n",
    "print(\"Second Largest:\", second)"
